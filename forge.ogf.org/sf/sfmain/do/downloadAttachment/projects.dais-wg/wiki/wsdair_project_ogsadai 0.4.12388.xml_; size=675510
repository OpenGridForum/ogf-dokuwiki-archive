    <wsdl:port name="ResponseServiceCoreDataAccessPT" binding="wsdaibindings:CoreDataAccessSOAP">
      <soap:address location="http://localhost:8080/axis/services/SQLResponseCoreServiceCoreDataAccessPT"/>
    </wsdl:port>
    <wsdl:port name="ResponseServiceCoreResourceListPT" binding="wsdaibindings:CoreResourceListSOAP">
      <soap:address location="http://localhost:8080/axis/services/SQLResponseServiceCoreResourceListPT"/>
    </wsdl:port>
