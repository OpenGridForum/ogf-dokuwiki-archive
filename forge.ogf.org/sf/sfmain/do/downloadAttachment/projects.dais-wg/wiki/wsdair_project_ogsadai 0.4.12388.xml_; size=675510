    <wsdl:part name="NotAuthorizedFault" element="wsdai:NotAuthorizedFault"/>
  </wsdl:message>
  <wsdl:message name="InvalidResourceNameFault">
    <wsdl:part name="InvalidResourceNameFault" element="wsdai:InvalidResourceNameFault"/>
  </wsdl:message>
