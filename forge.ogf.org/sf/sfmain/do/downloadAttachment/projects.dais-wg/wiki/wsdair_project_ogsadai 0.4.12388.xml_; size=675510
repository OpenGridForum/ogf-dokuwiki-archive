        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" ref="wsdair:AccessMode"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="SQLRowsetConfigurationDocument" substitutionGroup="wsdai:ConfigurationDocument" type="wsdair:SQLRowsetConfigurationDocumentType"/>
