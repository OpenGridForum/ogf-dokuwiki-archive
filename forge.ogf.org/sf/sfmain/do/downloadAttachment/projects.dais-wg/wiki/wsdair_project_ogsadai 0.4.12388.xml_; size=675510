<wsdl:definitions name="WS-DAIR-SQLAccess" targetNamespace="http://www.ggf.org/namespaces/2005/12/WS-DAIR" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:wsdai="http://www.ggf.org/namespaces/2005/12/WS-DAI" xmlns:wsdair="http://www.ggf.org/namespaces/2005/12/WS-DAIR" xmlns:wsdaibindings="http://www.ggf.org/namespaces/2005/12/WS-DAI/bindings" xmlns:wsdairbindings="http://www.ggf.org/namespaces/2005/12/WS-DAIR/bindings">
  <wsdl:import namespace="http://www.ggf.org/namespaces/2005/12/WS-DAI/bindings" location="../wsdai/dai_service_bindings.wsdl"/>
  <wsdl:import namespace="http://www.ggf.org/namespaces/2005/12/WS-DAIR/bindings" location="./coreaccess_bindings.wsdl"/>
  <wsdl:service name="SQLAccessService">
    <wsdl:port name="AccessServiceCoreDataAccessPT" binding="wsdaibindings:CoreDataAccessSOAP">
      <soap:address location="http://localhost:8080/axis/services/SQLAccessCoreServiceCoreDataAccessPT"/>
    </wsdl:port>
    <wsdl:port name="AccessServiceCoreResourceListPT" binding="wsdaibindings:CoreResourceListSOAP">
