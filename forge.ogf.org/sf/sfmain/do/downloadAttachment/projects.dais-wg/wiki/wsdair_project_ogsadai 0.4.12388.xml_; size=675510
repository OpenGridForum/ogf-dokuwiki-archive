      <wsdl:fault name="ServiceBusyFault">
        <soap:fault name="ServiceBusyFault" use="literal"/>
      </wsdl:fault>
    </wsdl:operation>
    <wsdl:operation name="Resolve">
      <soap:operation soapAction="http://www.ggf.org/namespaces/2005/12/WS-DAI#Resolve"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
      <wsdl:fault name="InvalidResourceNameFault">
        <soap:fault name="InvalidResourceNameFault" use="literal"/>
      </wsdl:fault>
