<xsd:schema targetNamespace="http://www.ggf.org/namespaces/2005/12/WS-DAIR" xmlns:wrs="http://java.sun.com/xml/ns/jdbc" xmlns:wsdai="http://www.ggf.org/namespaces/2005/12/WS-DAI" xmlns:wsdair="http://www.ggf.org/namespaces/2005/12/WS-DAIR" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import namespace="http://java.sun.com/xml/ns/jdbc" schemaLocation="../types/webrowset-jdbc150.xsd"/>
