        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="wsdai:RequestType">
              <xsd:sequence>
                <xsd:element ref="wsdair:SQLExpression"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="SQLExecuteResponse">
