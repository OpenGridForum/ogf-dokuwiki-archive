    <xsd:sequence>
      <xsd:element ref="wsdai:DataResourceDescription" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="wsdai:Readable" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="wsdai:Writeable" minOccurs="0" maxOccurs="1"/>
