\par Possible Applications
\par {\listtext\pard\plain\s2 \f1\fs28\insrsid8328695 \hich\af1\dbch\af0\loch\f1 1.\tab}}\pard \s2\ql \fi-360\li720\ri0\keepn\widctlpar\jclisttab\tx720\aspalpha\aspnum\faauto\ls6\outlinelevel1\adjustright\rin0\lin720\itap0\pararsid8328695 {\insrsid8328695 
WLCG Service Challenges}{\insrsid8328695\charrsid8328695 
\par }\pard\plain \ql \li0\ri0\widctlpar\aspalpha\aspnum\faauto\adjustright\rin0\lin0\itap0\pararsid8328695 \f1\fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\insrsid8328695\charrsid11433961 The WLCG Service Challenges (}{\field{\*\fldinst {
\insrsid8328695  HYPERLINK "}{\insrsid8328695\charrsid8328695 http://gridview.cern.ch/GRIDVIEW/}{\insrsid8328695 " }{\insrsid8328695 {\*\datafield 
00d0c9ea79f9bace118c8200aa004ba90b02000000170000002200000068007400740070003a002f002f00670072006900640076006900650077002e006300650072006e002e00630068002f00470052004900440056004900450057002f000000e0c9ea79f9bace118c8200aa004ba90b4400000068007400740070003a00
