\par {\listtext\pard\plain\f3\insrsid5920131\charrsid6763109 \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard \ql \fi-360\li720\ri0\widctlpar\jclisttab\tx720\tx3480\aspalpha\aspnum\faauto\ls24\adjustright\rin0\lin720\itap0 {
